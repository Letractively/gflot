<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder" xmlns:g="urn:import:com.google.gwt.user.client.ui">

  <ui:style>

  </ui:style>

  <ui:with type="com.googlecode.gflot.examples.client.resources.Resources" field="res"/>
  <ui:with type="com.googlecode.gflot.examples.client.mvp.NameTokens" field="tokens"/>

  <g:DockLayoutPanel unit="PX">
    <g:west size="200">
      <g:HTMLPanel>
        <g:Hyperlink ui:field="barLink" text="Bar" targetHistoryToken="{tokens.bar}" addStyleNames='{res.style.menuLink}'/>
        <g:Hyperlink ui:field="decimationLink" text="Decimation" targetHistoryToken="{tokens.decimation}" addStyleNames='{res.style.menuLink}'/>
        <g:Hyperlink ui:field="hoverLink" text="Hover" targetHistoryToken="{tokens.hover}" addStyleNames='{res.style.menuLink}'/>
        <g:Hyperlink ui:field="imageLink" text="Image" targetHistoryToken="{tokens.image}" addStyleNames='{res.style.menuLink}'/>
        <g:Hyperlink ui:field="interactiveLegendLink" text="Interactive Legend" targetHistoryToken="{tokens.interactiveLegend}" addStyleNames='{res.style.menuLink}'/>
        <g:Hyperlink ui:field="markingsLink" text="Markings" targetHistoryToken="{tokens.markings}" addStyleNames='{res.style.menuLink}'/>
        <g:Hyperlink ui:field="multipleAxesLink" text="Multiple axes" targetHistoryToken="{tokens.multiplesAxes}" addStyleNames='{res.style.menuLink}'/>
        <g:Hyperlink ui:field="overviewLink" text="Overview" targetHistoryToken="{tokens.overview}" addStyleNames='{res.style.menuLink}'/>
        <g:Hyperlink ui:field="pieLink" text="Pie" targetHistoryToken="{tokens.pie}" addStyleNames='{res.style.menuLink}'/>
        <g:Hyperlink ui:field="selectionLink" text="Selection" targetHistoryToken="{tokens.selection}" addStyleNames='{res.style.menuLink}'/>
        <g:Hyperlink ui:field="simpleLink" text="Simple" targetHistoryToken="{tokens.simple}" addStyleNames='{res.style.menuLink}'/>
        <g:Hyperlink ui:field="slidingLink" text="Sliding" targetHistoryToken="{tokens.sliding}" addStyleNames='{res.style.menuLink}'/>
        <g:Hyperlink ui:field="stackLink" text="Stack" targetHistoryToken="{tokens.stack}" addStyleNames='{res.style.menuLink}'/>
      </g:HTMLPanel>
    </g:west>
    <g:center>
      <g:SimpleLayoutPanel ui:field="container" />
    </g:center>
  </g:DockLayoutPanel>

</ui:UiBinder>