<?xml version="1.0" encoding="UTF-8"?>
<module rename-to='gflotexamples'>

  <inherits name="com.google.gwt.user.theme.chrome.Chrome" />
  <inherits name="com.google.gwt.activity.Activity" />
  <inherits name="com.google.gwt.place.Place" />

  <inherits name="ca.nanometrics.gflot.GFlot" />

  <!-- Generator to create the source code -->
  <generate-with class="com.googlecode.gflot.examples.generator.SourceGenerator">
    <when-type-assignable class="com.googlecode.gflot.examples.client.GFlotExamples.GeneratorInfo" />
  </generate-with>

  <entry-point class="com.googlecode.gflot.examples.client.GFlotExamples" />

</module>